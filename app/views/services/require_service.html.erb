<h2>Contratar Servicio</h2>

<%= form_for @contract, :url =>{:action => :require_service} do |f|%>
  
  <div class="field">
    <%= f.date_field :date_request %><br />

  </div>
 
  <div class="field">
    <%= f.label :description, 'Description' %><br />
    <%= f.text_area :description%>
  </div>

  <div class="actions">
    <%= f.submit "Contratar" %>
  </div>

<%end%> 